{
  "name": "<%= _.slugify(appname) %>",
  "version": "0.1.0",
  "description": "<%= _.capitalize(projectDesc) %>.",
  "license": "BSD",
  "homepage": "http://www.dephytech.com",
  "repository": "<%= githubUser %>/<%= _.slugify(appname) %>",
  "author": {
    "name": "<%= realname %>",
    "email": "<%= email %>",
    "url": "<%= githubUrl %>"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "test": "mocha"
  },
  "keywords": [
    "projects"
  ],
  "dependencies": {},
  "devDependencies": {
    "grunt": "~0.4.5",
    "load-grunt-tasks": "~0.6.0",
    <% if (!compass) { %>
    "node-sass": "~0.9.3",
    "grunt-sass": "~0.14.0",
    <% } else { %>
    "grunt-contrib-sass": "~0.8.0",
    <% } %>
    "grunt-contrib-watch": "~0.6.1",
    "grunt-contrib-clean": "~0.6.0",
    <% if (jade) { %>
    "grunt-contrib-jade": "~0.12.0",
    <% } %>
    "grunt-contrib-copy": "~0.5.0",
    "grunt-contrib-cssmin": "~0.10.0",
    "grunt-contrib-uglify": "~0.5.1",
    "grunt-contrib-concat": "~0.5.0",
    "grunt-contrib-jshint": "~0.10.0",
    "grunt-contrib-connect": "~0.8.0",
    "grunt-wiredep": "~1.8.0",
    "grunt-usemin": "~2.3.0",
    "grunt-newer": "~0.7.0"
  }
}
